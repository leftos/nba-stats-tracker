<Application x:Class="NBA_Stats_Tracker.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:players="clr-namespace:NBA_Stats_Tracker.Data.Players" StartupUri="Windows\MainWindow.xaml"
             DispatcherUnhandledException="App_DispatcherUnhandledException">
    <Application.Resources>
            <ObjectDataProvider x:Key="OptionsList" MethodName="GetValues"
                            ObjectType="{x:Type players:PlayerContractOption}">
                <ObjectDataProvider.MethodParameters>
                    <x:Type TypeName="players:PlayerContractOption" />
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
    </Application.Resources>
</Application>